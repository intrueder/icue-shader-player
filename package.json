{
  "name": "icue-shader-player",
  "productName": "iCUE Shader Player",
  "version": "0.2.1",
  "description": "Electron application to play GLSL shaders on Corsair keyboard",
  "scripts": {
    "build": "concurrently \"yarn run build-main\" \"yarn run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors --mode=development",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors --mode=production",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors --mode=production",
    "dev": "cross-env START_HOT=1 node -r babel-register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn run start-renderer-dev",
    "electron-rebuild": "electron-rebuild --parallel --force --types prod,dev,optional --module-dir app",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "yarn run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "postinstall": "node -r babel-register internals/scripts/CheckNativeDep.js && yarn run build-dll && electron-builder install-app-deps && electron-rebuild --force && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prestart": "yarn run build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js --mode=development",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && electron-builder --win --x64"
  },
  "browserslist": "electron 6.0",
  "build": {
    "productName": "iCUE Shader Player",
    "appId": "com.intrueder.icueshaderplayer",
    "win": {
      "target": [
        "7z"
      ]
    },
    "files": [
      "dist/",
      "scripts/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "extraResources": [
      {
        "from": "./app/shaders/",
        "to": "shaders",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./app/images/",
        "to": "images",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.2",
    "babel-loader": "7.1.5",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-dynamic-import-webpack": "1.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-classes": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.4.2",
    "concurrently": "4.1.1",
    "cross-env": "5.2.0",
    "cross-spawn": "6.0.5",
    "css-loader": "3.1.0",
    "detect-port": "1.3.0",
    "electron": "6.0.0",
    "electron-builder": "21.1.5",
    "electron-debug": "3.0.1",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.8.5",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-formatter-pretty": "2.1.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-compat": "3.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "fbjs-scripts": "1.2.0",
    "file-loader": "4.1.0",
    "identity-obj-proxy": "3.0.0",
    "minimist": "1.2.3",
    "node-sass": "4.12.0",
    "npm-logical-tree": "1.2.1",
    "redux-logger": "3.0.6",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "terser-webpack-plugin": "1.4.3",
    "url-loader": "2.1.0",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "devtron": "1.4.0",
    "font-awesome": "4.7.0",
    "raf": "3.4.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.12.9",
    "react-redux": "7.1.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "source-map-support": "0.5.12"
  },
  "devEngines": {
    "node": ">=10.x",
    "npm": ">=6.x",
    "yarn": ">=1.16.x"
  }
}
